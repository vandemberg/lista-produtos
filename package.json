{
  "name": "lista-produtos",
  "version": "1.0.0",
  "description": "Sistema para listar produtos",
  "main": "server.js",
  "scripts": {
    "client": "cd client && npm start",
    "server": "node server.js",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "seed": "node db-seed.js",
    "start" : "cd client && npm run build && cd .. && npm run seed && npm run server"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.16.2",
    "mongo-seeding": "^3.0.0",
    "mongoose": "^5.3.15",
    "mongoose-seed": "^0.5.1",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "concurrently": "^3.5.0"
  },
  "keywords": [
    "nodejs",
    "node.js",
    "react",
    "react.js",
    "express",
    "mongodb"
  ],
  "author": "Vandemberg Lima",
  "license": "ISC"
}
